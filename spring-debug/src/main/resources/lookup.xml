<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="apple" class="com.wy.methodOverides.lookup.Apple" scope="prototype"></bean>
	<bean id="banana" class="com.wy.methodOverides.lookup.Banana" scope="prototype"></bean>

	<bean id="fruitPlate1" class="com.wy.methodOverides.lookup.FruitPlate">
		<lookup-method name="getFruit" bean="apple"></lookup-method>
	</bean>

	<bean id="fruitPlate2" class="com.wy.methodOverides.lookup.FruitPlate">
		<lookup-method name="getFruit" bean="banana"></lookup-method>
	</bean>
</beans>